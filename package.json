{
  "name": "election-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=develop electron .",
    "dev": "NODE_ENV=develop cd ../markdown_table_creator && npm run electron && cd ../electron-quick-start && electron .",
    "build": "NODE_ENV=product electron-packager . 日记生成器  --platform=mas --arch=all  --out=output/ --overwrite --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=output --prune",
    "deploy": "npm run build && cp -rfp output/日记生成器-mas-x64/日记生成器.app /Applications/ && echo '部署成功...' && open /Applications/日记生成器.app"
  },
  "repository": "",
  "keywords": [
    "Electron",
    "justyan"
  ],
  "author": "justyan",
  "license": "MIT",
  "devDependencies": {
    "electron": "^2.0.0"
  },
  "dependencies": {
    "cron-parser": "^2.5.0",
    "mongoose": "^5.2.2",
    "node-schedule": "^1.3.0"
  }
}
